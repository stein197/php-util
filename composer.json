{
    "name": "stein197/util",
    "description": "Utility functions",
    "version": "1.1.2",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "phpunit --display-deprecations --display-notices --display-warnings test.php",
        "test:function_track": "phpunit test.php --filter function_track_ --display-deprecations --display-notices --display-warnings",
        "test:iterate": "phpunit test.php --filter iterate_ --display-deprecations --display-notices --display-warnings",
        "test:property_exists": "phpunit test.php --filter property_exists_ --display-deprecations --display-notices --display-warnings",
        "test:property_get": "phpunit test.php --filter property_get_ --display-deprecations --display-notices --display-warnings",
        "test:property_list": "phpunit test.php --filter property_list_ --display-deprecations --display-notices --display-warnings",
        "test:property_set": "phpunit test.php --filter property_set_ --display-deprecations --display-notices --display-warnings",
        "test:property_unset": "phpunit test.php --filter property_unset_ --display-deprecations --display-notices --display-warnings",
        "test:to_array": "phpunit test.php --filter to_array_ --display-deprecations --display-notices --display-warnings",
        "test:to_object": "phpunit test.php --filter to_object_ --display-deprecations --display-notices --display-warnings",
    },
    "scripts-descriptions": {
        "test": "Run tests"
    },
    "autoload": {
        "psr-4": {
            "Stein197\\Util\\": "src/"
        },
        "files": [
            "index.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
